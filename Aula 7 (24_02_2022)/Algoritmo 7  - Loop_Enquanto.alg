algoritmo "Loop Enquanto"
// Função : Repete até atingir uma condição

// Data : 24/02/2022

var
   palavra:caractere
   contador:inteiro

inicio
   palavra <- "Senac - Algoritimo"
   contador <- 1
   
   enquanto (contador <=10) faca
      escreval(palavra)
      
   //Atualização da variável
   
   contador <- contador + 1
   
   fimenquanto

fimalgoritmo
