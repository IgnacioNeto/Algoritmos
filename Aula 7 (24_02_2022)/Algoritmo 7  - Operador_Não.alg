algoritmo "Operador Não (Negação)"
// Função : Inverte a lógica

// Data : 24/02/2022

var
   blackFriday: logico //Verdadeiro ou falso

inicio
   blackFriday <- falso
   
   se nao(blackFriday) entao
      escreval("Não estamos na black friday... :(")
   
   senao
      escreval("Estamos na black friday... :)")
   
   fimse


fimalgoritmo
